---
import "../assets/css/index.scss";
import Meta from "../config/Meta.astro";
const { pageData } = Astro.props;
---

<!doctype html>
<html lang="ja">
  <head>
    <Meta pageMeta={Astro.props.pageMeta} />
  </head>
  <body data-page={pageData}>
    <slot />
    <script src="../assets/js/index.js"></script>
  </body>
</html>
