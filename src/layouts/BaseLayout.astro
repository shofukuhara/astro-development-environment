---
import '../css/index.scss';
import Meta from '../components/config/Meta.astro';
const { pageData } = Astro.props;
---

<!doctype html>
<html lang="ja">
  <head>
    <Meta pageMeta={Astro.props.pageMeta} />

    <script src="../js/index.js"></script>
  </head>
  <body data-page={pageData}>
    <slot />
  </body>
</html>
